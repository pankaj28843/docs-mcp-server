      "url_blacklist_prefixes": "https://docs.djangoproject.com/en/5.2/releases/",
      "enable_crawler": false,
      "docs_root_dir": "./mcp-data/django",
      "refresh_schedule": "0 2 */14 * *",
      "test_queries": {
        "natural": [
          "How to create a Django model with foreign key",
